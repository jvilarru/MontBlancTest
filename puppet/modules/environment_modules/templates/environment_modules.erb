trap "" 1 2 3

case "$0" in
-bash|bash|*/bash) . <%= @prefix %>/Modules/default/init/bash ;;
    -ksh|ksh|*/ksh) . <%= @prefix %>/Modules/default/init/ksh ;;
    -zsh|zsh|*/zsh) . <%= @prefix %>/Modules/default/init/zsh ;;
    *) . <%= @prefix %>/Modules/default/init/sh ;; # sh and default for scripts
esac

trap 1 2 3
